 import scala.language.postfixOps
 import scala.annotation.tailrec
 go()

 def go() {

   //zadanie 1
  
   def f1(n: String): String = {
     n match{
       case "Poniedzialek"|"Wtorek"|"Sroda"|"Czwartek"|"Piatek"=> "Praca"
       case "Sobota"|"Niedziela" => "Weekend"
       case other => "Nie ma takiego dnia"
     }
   }
   
   print(f1("Wtorek"))
 }
